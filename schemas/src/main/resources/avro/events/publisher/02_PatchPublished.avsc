{
  "name": "PatchPublished",
  "namespace": "org.pops.et4.jvm.project.schemas.events",
  "doc": "Event emitted when a patch is published",
  "type": "record",
  "fields": [
    {
      "name": "gameId",
      "doc": "ID of the game concern by the patch",
      "type": "long",
      "default": 0
    },
    {
      "name": "version",
      "doc": "New version of the game",
      "type": "string",
      "default": "1.0.0"
    }
  ]
}
